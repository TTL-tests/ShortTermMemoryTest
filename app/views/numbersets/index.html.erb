<h1>Listing numbersets</h1>

<table>
  <thead>
    <tr>
      <th>Position</th>
      <th>List</th>
      <th>Length</th>
      <th>Order</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @numbersets.each do |numberset| %>
      <tr>
        <td><%= numberset.position %></td>
        <td><%= numberset.list_id %></td>
        <td><%= numberset.length %></td>
        <td><%= numberset.order %></td>
        <td><%= link_to 'Show', numberset %></td>
        <td><%= link_to 'Edit', edit_numberset_path(numberset) %></td>
        <td><%= link_to 'Destroy', numberset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Numberset', new_numberset_path %>
